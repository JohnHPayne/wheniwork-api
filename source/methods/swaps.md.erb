    ],
    "users" : [
    <%= print_json(data.objects['user'], :minimal=>true, :include=>{'id'=>134,'first_name'=>'Emmett','last_name'=>'Brown'}).indent(4) %>,
    <%= print_json(data.objects['user'], :minimal=>true, :include=>{'id'=>132,'first_name'=>'Lorraine','last_name'=>'Tannen'}).indent(4) %>,
